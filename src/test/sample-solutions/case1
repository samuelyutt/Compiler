<KWbegin>
1: begin
<KWvar>
<id: a>
<:>
<KWinteger>
<;>
2: 	var a: integer;
<KWvar>
<id: b>
<:>
<KWreal>
<;>
3: 	var b: real;
<KWvar>
<id: c>
<:=>
<float: 1.23>
<;>
4: 	var c:= 1.23;
<id: a>
<:=>
<integer: 1>
<+>
<integer: 1>
<;>
5: 	a:= 1+1;
<id: b>
<:=>
<float: 1.23>
<;>
6: 	b:= 1.23;
<id: c>
<:=>
<oct_integer: 00>
<float: 9.1>
<;>
7: 	c:= 009.1;
<KWend>
8: end
