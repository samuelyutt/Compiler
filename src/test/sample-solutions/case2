<KWbegin>
1: begin
<KWvar>
<id: a>
<:>
<KWreal>
<;>
2: 	var a: real;
<KWvar>
<id: b>
<:>
<scientific: 123E+4>
<;>
3: 	var b: 123E+4;
<KWvar>
<id: c>
<:>
<KWstring>
<;>
4: 	var c: string;
<id: a>
<:=>
<scientific: 1.23E4>
<;>
5: 	a:= 1.23E4;
<id: b>
<:=>
<oct_integer: 00123>
<id: E4>
<;>
6: 	b:= 00123E4;
<id: c>
<:=>
<string: test>
<;>
7: 	c:= "test";
<KWend>
8: end
